{
  "name": "benjaminbowles/gocardless-laravel",
  "description": "GoCardless Pro PHP Client package integration for Laravel.",
  "keywords": [
    "php",
    "Benjaminbowles",
    "laravel",
    "gocardless"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Bowles",
      "email": "ben@benbowles.co.uk",
      "homepage": "https://benbowles.co.uk"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": ">=5.5.0",
    "gocardless/gocardless-pro": "^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Benjaminbowles\\Gocardless\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Benjaminbowles\\Gocardless\\GocardlessServiceProvider"
      ],
      "aliases": {
        "Gocardless": "Benjaminbowles\\Gocardless\\Facades\\Gocardless"
      }
    }
  },
  "minimum-stability": "stable"
}
